// Preferences: Open Keyboard Shortcuts (JSON)
[
    {
        "key": "ctrl+d",
        "command": "extension.multiCommand.execute",
        "args": { "command": "CtrlDCombo" },
        "when": "editorTextFocus && neovim.mode == 'normal'"
    },
    {
        "key": "ctrl+f",
        "command": "extension.multiCommand.execute",
        "args": { "command": "CtrlFCombo" },
        "when": "editorTextFocus && neovim.mode == 'normal'"
    },
]
